eclipse_distro: luna
eclipse_package: java

# manipulate eclipse.ini
eclipse_ini_overwrite: true
eclipse_ini_flags_next_line:
  "--launcher.XXMaxPermSize": 256m
eclipse_ini_flags_vmargs:
  #default is 256m
  "-XX:MaxPermSize=": "1024m"
  #default is -Xms40m
  "-Xms": "512m"
  #default is -Xmx512m
  "-Xmx": "2048m"

